<!DOCTYPE html>
<html lang="ja">
    <head>
        <meta http-equiv="content-type" content="text/html; charset=UTF-8">
        <title><%= title %></title>
        <link rel='stylesheet' href='/stylesheets/style.css' />
    </head>
    <body>
        <head><h1><%= title %></h1></head>
        <div role="main">
            <table>
                <tr><th>ID</th><th>NAME</th><th>MAIL</th></tr>
                <% content.forEach(function (value, key) { %>
                <tr>
                    <td><%= value.ID %></td>
                    <td><%= value.NAME %></td>
                    <td><%= value.MAIL %></td>
                </tr>
                <% }); %>
            </table>
        </div>
    </body>
 </html>